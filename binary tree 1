#class tree that contains the basic elements of tree
class Tree(obj):
    def __init__(self):
        self.right = None
        self.left = None
        self.data = None
        
root = Tree()
root.data = "root"
root.left = Tree()
root.left.data = "left data"
root.right = Tree()
root.right.data = "right data"

print("Left data", root.left.data)
print("Root data", root.data)
print("Right data", root.right.data)

